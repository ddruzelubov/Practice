		{
			"name": "Auth",
			"item": [
				{
					"name": "POST Register",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", () => pm.response.to.have.status(201));",
									"pm.test(\"Response has accessToken and user.id\", () => {",
									"    const json = pm.response.json();",
									"    pm.expect(json.accessToken).to.be.a('string');",
									"    pm.expect(json.user.id).to.be.a('string');",
									"    pm.environment.set(\"authToken\", json.accessToken);",
									"    pm.environment.set(\"userId\", json.user.id);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"fituser2025\",\n  \"email\": \"fituser2025@example.com\",\n  \"password\": \"SecurePass123\"\n}"
						},
						"url": "{{baseUrl}}/register",
						"description": "Регистрация нового пользователя"
					},
					"response": []
				},
				{
					"name": "POST Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", () => pm.response.to.have.status(200));",
									"pm.test(\"Token saved to environment\", () => {",
									"    const json = pm.response.json();",
									"    pm.expect(json.accessToken).to.be.a('string');",
									"    pm.environment.set(\"authToken\", json.accessToken);",
									"    pm.environment.set(\"userId\", json.user.id);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"fituser2025@example.com\",\n  \"password\": \"SecurePass123\"\n}"
						},
						"url": "{{baseUrl}}/login",
						"description": "Вход в систему — автоматически сохраняет токен"
					},
					"response": []
				}
